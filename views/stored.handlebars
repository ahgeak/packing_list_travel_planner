<div id="stored" class="row center-align">

    <h3>Stored List</h3>


    <div class="stored_list col s12">
        {{#each lists as |list|}}
        <div class="col s12">
            <a id="list_{{@index}}" class="btn-small">{{list.name}}</a>
            <ol>

                {{#each list.items}}
                <li>{{list.items}}</li>
                {{/each}}
            </ol>

        </div>
        {{/each}}
