<div class="row center-align">
    <div class="input-field col s12" id="list_name">
        <input placeholder="Enter your List name" id="list_name" type="text" class="validate">
    </div>
    <div  class="list_image col s12">
        <img src="/images/create-bg.jpg" />
    </div>

    <div id="item_list" class=" col s12">
        <div class="input-field col s12" id="input_items">
            <input placeholder="Enter your Items" id="input_items" type="text" class="validate">
        </div>
        <div id="items" class="col s12">
            <a class="btn-floating btn-medium waves-effect waves-teal"><i class="material-icons">add</i></a>
        </div>
         <div id="add_list" class="col s12">
            <a class="btn-large waves-effect waves-teal"><i class="fa-regular fa-floppy-disk"></i>&nbsp;&nbsp;&nbsp;Save my List</i></a>
        </div>
    </div>
</div>
</div>

<script src="https://kit.fontawesome.com/a3bce9e655.js" crossorigin="anonymous"></script>